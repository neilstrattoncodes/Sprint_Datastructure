<% for(var i = 0; i < games.length; i++) { %>
<tr>
  <td><%= i + 1 %></td>
  <td><%= games[i].name %></td>
  <td><%= games[i].desc %></td>
  <td><%= games[i].type %></td>
  <td><%= games[i].price %></td>
  <td><%= games[i].sku %></td>
  <td><%= games[i].platform %></td>
  <td><%= games[i].status %></td>
  <td>
    <a
      href="/update-game?id=<%= games[i]._id%>"
      class="btn border-shadow update"
    >
      <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
    </a>
    <a class="btn border-shadow delete" data-id="<%=games[i]._id%>">
      <span class="text-gradient"><i class="fas fa-times"></i></span>
    </a>
  </td>
</tr>
<% } %>
